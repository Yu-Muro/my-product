{
    "mcpServers": {
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "cloudflare-docs": {
            "command": "npx",
            "args": [
                "mcp-remote",
                "https://docs.mcp.cloudflare.com/sse"
            ]
        },
        "cloudflare-bindings": {
            "command": "npx",
            "args": [
                "mcp-remote",
                "https://bindings.mcp.cloudflare.com/sse"
            ]
        },
        "cloudflare-observability": {
            "command": "npx",
            "args": [
                "mcp-remote",
                "https://observability.mcp.cloudflare.com/sse"
            ]
        }
    }
}
